<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CalcularServlet</servlet-name>
  	<display-name>CalcularServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.CalcularServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SoloTextoGenerado</servlet-name>
  	<display-name>SoloTextoGenerado</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.SoloTextoGenerado</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CalcularConExcepcionesServlet</servlet-name>
  	<display-name>CalcularConExcepcionesServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.CalcularConExcepcionesServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CalcularConExcepcionesB</servlet-name>
  	<display-name>CalcularConExcepcionesB</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.CalcularConExcepcionesB</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CalcularServlet</servlet-name>
  	<url-pattern>/CalcularServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SoloTextoGenerado</servlet-name>
  	<url-pattern>/SoloTextoGenerado</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CalcularConExcepcionesServlet</servlet-name>
  	<url-pattern>/CalcularConExcepcionesServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CalcularConExcepcionesB</servlet-name>
  	<url-pattern>/CalcularConExcepcionesB</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>login</welcome-file>
  </welcome-file-list>
</web-app>
